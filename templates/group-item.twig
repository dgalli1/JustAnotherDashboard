<a class="block text-boxcontent mb-2 p-1 pl-4 bg-groupelement h-8" href="{{ url }}">
    {% if logo_file %}
        <img src="/res/images/{{ logo_file }}" class="inline-block w-8 mr-4">
    {% elseif logo_icon %}
        <i class="gg-mail"></i>
    {% else %}
        <span class="inline-block w-12">&nbsp;</span>
    {% endif %}
    {{ text }}
</a>
